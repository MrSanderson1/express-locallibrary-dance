doctype html
html
    head
        title Move Dancecenter
    body(style="padding-top: 90px;")
        block content
            // Navbar (sit on top)
            include navigation
            // Header
            header#home.w3-display-container.w3-content.w3-wide(style='max-width:1500px;')
                img.w3-image(src='/images/move4eventslogo.png' alt='Move dancecenter' width='1500' height='800')
                .w3-display-middle.w3-margin-top.w3-center
                    h1.w3-xxlarge.w3-text-white
                        span.w3-padding.w3-black.w3-opacity-min
                            b Move
                        span.w3-hide-small.w3-text-light-grey Dancecenter
            // Page content
            .w3-content.w3-padding(style='max-width:1564px')
                // Project Section
                include lessen
                // About Section
                include over
                // Contact Section
                include contact
                //end of page content